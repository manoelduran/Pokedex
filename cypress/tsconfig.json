{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "ES2016.Array.Include"],
      "types": ["cypress"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "noImplicitAny": false,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["**/*.ts"]
  }